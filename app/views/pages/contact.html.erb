<!DOCTYPE html>
<html lang="en">
<body data-aos-easing="ease-out-back" data-aos-duration="1000" data-aos-delay="0" style>
	<!-- banner -->
	<header>
		<!-- navigation -->
		<div class="main-top py-1 aos-init aos-animate" data-aos="fade-down">
			<nav class="navbar navbar-expand-lg navbar-light">
				<div class="container-fluid">
					<!-- logo -->
					<%= link_to image_tag("/assets/igire.png",  alt: "igire", class: "img-fluid", id: "logo"), index_path, class: "navbar-brand font-weight-bold" %>
					<!-- //logo -->
					<button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="navbar-collapse text-center collapse" id="navbarSupportedContent" style="">
						<ul class="navbar-nav ml-lg-auto">
							<li class="nav-item active">
								<%= link_to "Home", index_path, class: "nav-link text-uppercase"%>
							</li>
							<li class="nav-item dropdown">
								<a class="nav-link dropdown-toggle text-uppercase" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
									About Us
								</a>
								<div class="dropdown-menu" aria-labelledby="navbarDropdown" style="display: none;">
									<%= link_to "Our Organization", about_path, class: "dropdown-item text-uppercase list-group-item"  %>
									<%= link_to "Our Team", team_path, class: "dropdown-item text-uppercase list-group-item"  %>
								</div>
							</li>
							<li class="nav-item dropdown">
								<a class="nav-link dropdown-toggle text-uppercase" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
									Programs
								</a>
								<div class="dropdown-menu" aria-labelledby="navbarDropdown" style="display: none;">
									<%= link_to "SheCanCode", shecancode_path, class: "dropdown-item text-uppercase list-group-item"  %>
									<a class="dropdown-item text-uppercase list-group-item" href="#stats">Girls in ICT</a>
									<a class="dropdown-item text-uppercase  list-group-item" href="#">Kufandisha Rwanda Fellowship</a>
								</div>
							</li>
							<li class="nav-item">
								<%= link_to "Contact Us", contact_path, class: "nav-link text-uppercase"  %>
							</li>
							<li class="nav-item ml-lg-5 mt-lg-0 mt-sm-5 mt-4">
								<a href="#" class="reqe-button text-uppercase">Donate</a>
							</li>
						</ul>
					</div>
				</div>
			</nav>
		</div>
		<!-- //navigation -->
	</header>
	<!-- //header -->
	<div class="pages">
	</div>
	<!-- //banner -->

	<section class="contact py-5" id="contact">
		<div class="container py-xl-5 py-lg-3">
			<!-- map -->
			<div class="col-lg-12 map aos-init aos-animate" data-aos="flip-left" data-aos-easing="ease-out-cubic" data-aos-duration="2000">
				<iframe  src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJRaq3WRen3BkRhXzRmqJQ3iw&key=AIzaSyCXJoXjURY8_KXLfPJuZfS1l1SsgubR-WI" allowfullscreen = "True"></iframe>
			</div>
			<!-- //map -->
		</div>


		<!-- contact -->
		<div class="container py-xl-5 py-lg-3">
			<div class="row mb-lg-4">
				<div class="col-lg-6 aboutright mt-xl-0 mt-lg-5 aos-init aos-animate" data-aos="fade-up">
					<h3 class="tittle text-dark font-weight-bold">Get Involved</h3>
					<p>Interested in learning more about us or getting involved? Send us an email and we'll get back to you as soon as we can!</p>
					<div class="main_grid_contact contact-form">
						<form action="#" method="post">
							<div class="form-group">
								<input class="form-control" type="text" name="Name" placeholder="Name" required="">
							</div>
							<div class="form-group">
								<input class="form-control" type="email" name="Email" placeholder="Email" required="">
							</div>
							<div class="form-group">
								<textarea id="textarea" name="message" placeholder="Message" required=""></textarea>
							</div>
							<div class="input-group1">
								<input class="form-control" type="submit" value="Submit">
							</div>
						</form>
					</div>
				</div>
				<div class="col-lg-5 aboutleft ml-xl-4 mt-lg-3 mt-4 aos-init aos-animate" data-aos="fade-up">
					<h3 class="tittle text-dark font-weight-bold mb-4">Stay Connected</h3>
					<p>Follow us on social media to stay updated on the latest IRO news!</p>
					<a class="twitter-timeline" data-height="300" href="https://twitter.com/igirerwandaorg?ref_src=twsrc%5Etfw">Tweets by igirerwandaorg</a> <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
				</div>
			</div>
		</section>
		<!-- //contact -->
		<!-- menu js -->
		<script>
			$('.toggle').on('click', function() {
				$('.menu').toggleClass('open');
				$('.w3nav').toggleClass('show-w3nav');
				$('.w3nav a').toggleClass('show-w3nav-link');
				$('.toggle').toggleClass('close');
				$('.header-icon').toggle(800);
			});
		</script>
		<!-- //menu js -->
		<!-- start-smooth-scrolling -->
		<script type="text/javascript" src="js/move-top.js"></script>
		<script type="text/javascript" src="js/easing.js"></script>
		<script type="text/javascript">
			jQuery(document).ready(function($) {
				$(".scroll").click(function(event){
					event.preventDefault();

					$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
				});
			});
		</script>
		<!-- //end-smooth-scrolling -->
		<!-- smooth-scrolling-of-move-up -->
		<script type="text/javascript">
			$(document).ready(function() {
				/*
				var defaults = {
				containerID: 'toTop', // fading element id
				containerHoverID: 'toTopHover', // fading element hover id
				scrollSpeed: 1200,
				easingType: 'linear'
			};
			*/

			$().UItoTop({ easingType: 'easeOutQuart' });

		});
	</script>
</body>
</html>
